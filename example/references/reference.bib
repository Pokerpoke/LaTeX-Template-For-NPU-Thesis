@article{dawkins_single-chip_2003,
	title = {A single-chip tuner for {DVB}-T},
	volume = {38},
	issn = {0018-9200},
	doi = {10.1109/JSSC.2003.814436},
	abstract = {A new architecture is presented for a single-chip tuner for digital terrestrial television, based on existing double conversion and direct conversion topologies. The new design forms part of a mixed-signal Digital Video Broadcasting-Terrestrial ({DVB}-T) receiver system, employing digital signal processing at baseband to ensure minimal performance requirements for the analog circuitry. To evaluate the potential performance of this new tuner/receiver system, high-level system simulations have been performed, followed by the construction of a prototype {DVB}-T receiver using a custom-designed analog {ASIC} which integrates all analog tuner blocks (including channel filtering) on one chip. Measured results from this chip, implemented in a 20-{GHz} bipolar technology, show an overall third-order input referred intercept point of 116 {dBμV}, a noise figure of 14 {dB} and an automatic gain control range of 71.4 {dB}, drawing 250 {mA} at a 5-V supply.},
	pages = {1307--1317},
	number = {8},
	journaltitle = {{IEEE} Journal of Solid-State Circuits},
	author = {Dawkins, M. and Burdett, A. P. and Cowley, N.},
	date = {2003-08},
	keywords = {5 V, 14 {dB}, 20 {GHz}, 250 {mA}, application specific integrated circuits, automatic gain control, Baseband, channel filtering, Circuit simulation, Circuit topology, consumer electronics, custom-designed analog {ASIC}, digital signal processing, digital terrestrial television, digital video broadcasting, direct conversion, double conversion, {DVB}-T, high-level system simulations, mixed-signal Digital Video Broadcasting-Terrestrial receiver system, noise figure, Performance evaluation, Signal design, single-chip tuner, third-order input referred intercept point, Tuners, tuning, {TV} broadcasting, Virtual prototyping}
}